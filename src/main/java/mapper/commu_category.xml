<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.commu_category">
<!--  	<resultMap id="CategoryResult" type="dto.CommuCategoryDto">
        <id property="categoryNo" column="category_no"/>
        <result property="categoryName" column="category_name"/>
    </resultMap>  -->
	<select id="selectAll" resultType="commuCategory"> 
		select category_no, category_name from commu_category 
	</select>
	
	<select id="selectByNo" resultType='commuCategory'> <!-- 어떤 타입인지 받아온다 -->
		select category_no, category_name from commu_category where category_no = #{categoryNo}
	</select>
</mapper>
